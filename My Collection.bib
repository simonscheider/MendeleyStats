doi = {10.1249/MSS.0b013e3181877aaf},
isbn = {01959131},
issn = {0195-9131},
journal = {Medicine and Science in Sports and Exercise},
keywords = {A,Bicycling,Curvimeter,Gps,M:ok,Reproducibility,Sweden,Validity,Walking,adult,article,bicycle,cycling,distance perception,female,global positioning system,human,human experiment,male,questionnaire,validation study,validity,walking},
