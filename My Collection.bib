keywords = {Big data,Cycling,GIS,Location-based services (LBSs),Privacy,Urban planning},
month = {oct},
pages = {135--144},
title = {{Methods for deriving and calibrating privacy-preserving heat maps from mobile sports tracking application data}},
url = {http://www.sciencedirect.com/science/article/pii/S0966692315001647},
volume = {48},
year = {2015}
