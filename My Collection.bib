journal = {Physical therapy},
keywords = {Aged,Amputation,Amputation: psychology,Amputation: rehabilitation,Artificial Limbs,Automobiles,Female,Geographic Information Systems,Humans,Interpersonal Relations,Leg,Leg: surgery,Outcome Assessment (Health Care),Outcome Assessment (Health Care): methods,Peripheral Vascular Diseases,Peripheral Vascular Diseases: surgery,Social Participation,Walking,Wheelchairs},
month = {mar},
