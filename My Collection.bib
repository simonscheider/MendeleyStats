pages = {25--32},
pmid = {16580251},
title = {{Comparison of global positioning and computer-based tracking systems for measuring player movement distance during Australian Football}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-33646188767{\&}partnerID=tZOtx3y1 http://www.sciencedirect.com/science/article/pii/S1440244006000053},
