journal = {Human Movement Science},
keywords = {Eccentric,Fatigue,Football,GPS,Motion analysis,Muscle damage},
month = {mar},
pages = {96--101},
pmid = {26615476},
title = {{Relationships between match activities and peak power output and Creatine Kinase responses to professional reserve team soccer match-play}},
url = {http://www.sciencedirect.com/science/article/pii/S0167945715300610},
volume = {45},
