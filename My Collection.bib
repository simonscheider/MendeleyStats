journal = {American Journal of Preventive Medicine},
keywords = {Accelerometry,Accelerometry: methods,Child,England,Female,Geographic Information Systems,Humans,Male,Motor Activity,Motor Activity: physiology,Schools,Sex Factors,Time Factors,Walking,Walking: physiology},
month = {aug},
number = {2},
pages = {201--204},
pmid = {22813686},
title = {{Contribution of the school journey to daily physical activity in children aged 11-12 years}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84864128231{\&}partnerID=tZOtx3y1},
