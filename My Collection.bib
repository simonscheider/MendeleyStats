journal = {International journal of health geographics},
keywords = {Aged,Aged, 80 and over,Behavioral Research,Cross-Sectional Studies,Environment Design,Female,Geographic Information Systems,Health Behavior,Humans,Likelihood Functions,Male,Odds Ratio,Residence Characteristics,Walking},
