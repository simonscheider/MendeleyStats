mendeley-tags = {A},
month = {jul},
number = {6},
pages = {803--807},
title = {{Bladder filling reduces femoral artery wall distension and strain: Beware of a full bladder!}},
url = {http://www.sciencedirect.com/science/article/pii/S0301562998000362},
