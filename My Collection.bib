}
@article{Ross2015,
abstract = {The purpose of this study was to quantify the global match demands of international rugby sevens and to compare the match$\backslash$r$\backslash$ndemands of forwards and backs, and between tournament rounds. To assess the match demands, global positioning system$\backslash$r$\backslash$n(GPS) and video analysis were collected from 27 international rugby sevens players from the same team across an entire$\backslash$r$\backslash$nInternational Rugby Board Sevens World Series season. Differences in running demands and match activities between$\backslash$r$\backslash$nforwards and backs were mostly trivial and small (ES = 0.05–0.84) while differences in running demands and match$\backslash$r$\backslash$nactivities between Pool and Cup rounds were trivial (ES = 0.001–0.12). Cup round matches showed an increase in longduration$\backslash$r$\backslash$nball-in-play sequences (proportion ratio 0.46). These findings suggest international rugby sevens forwards and$\backslash$r$\backslash$nbacks experience similar match demands while overall match demands remain consistent across tournament rounds.},
