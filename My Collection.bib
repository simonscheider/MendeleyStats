pmid = {25611681},
publisher = {Human Kinetics Publishers Inc.},
title = {{Running-intensity fluctuations in elite rugby sevens performance}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84939787027{\&}partnerID=tZOtx3y1},
