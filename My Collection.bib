title = {{Fitness level and physical capacity of commuter cyclists and the actual condition while cycling to work in Tokai region}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84860464856{\&}partnerID=tZOtx3y1},
volume = {61},
year = {2012}
