journal = {Journal of Science and Medicine in Sport},
keywords = {Drafting,Elite,Garmin Etrex Vista,Gender,Panasonic NV-MX300EG,Performance,Ranking,Triathlete,article,controlled study,cycling,female,global positioning system,human,male,normal human,physical performance,running,sex difference,swimming,triathlon,velocity,videorecording},
