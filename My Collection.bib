@article{Jian2015,
abstract = {This paper presents information on a portable fall detection and$\backslash$nalerting system mainly consisting of a custom vest and a mobile smart$\backslash$nphone. A wearable motion detection sensor integrated with tri-axial$\backslash$naccelerometer, gyroscope and Bluetooth is built into a custom vest worn$\backslash$nby elderly. The vest can capture the reluctant acceleration and angular$\backslash$nvelocity about the activities of daily living (ADLs) of elderly in real$\backslash$ntime. The data via Bluetooth is then sent to a mobile smart phone$\backslash$nrunning a fall detection program based on k-NN algorithm. When a fall$\backslash$noccurs the phone can alert a family member or health care center through$\backslash$na call or emergent text message using a built in Global Positioning$\backslash$nSystem. The experimental results show that the system discriminates$\backslash$nfalls from ADLs with a sensitivity of 95{\%}, and a specificity of$\backslash$n96.67{\%}. This system can provide remote monitoring and timely help for$\backslash$nthe elderly.},
