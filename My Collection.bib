title = {{Combining GPS, GIS, and accelerometry: methodological issues in the assessment of location and intensity of travel behaviors.}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-74949130920{\&}partnerID=tZOtx3y1},
volume = {7},
